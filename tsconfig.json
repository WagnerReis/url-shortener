{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "types": [
      "vitest/globals"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@app/cryptography": [
        "libs/cryptography/src"
      ],
      "@app/cryptography/*": [
        "libs/cryptography/src/*"
      ],
      "@env/env": [
        "libs/env/src"
      ],
      "@env/env/*": [
        "libs/env/src/*"
      ],
      "@prisma-lib": [
        "libs/prisma/src"
      ],
      "@prisma-lib/*": [
        "libs/prisma/src/*"
      ]
    }
  }
}